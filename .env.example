# Friendship AI Backend - Environment Variables

# Uygulama
DEBUG=true

# OpenAI API
OPENAI_API_KEY=sk-your-openai-api-key-here

# Veritabanı (PostgreSQL)
DATABASE_URL=postgresql+asyncpg://user:password@localhost:5432/friendship_ai

# Redis
REDIS_URL=redis://localhost:6379

# JWT
JWT_SECRET_KEY=your-super-secret-jwt-key-change-in-production

# Analiz Ayarları
MIN_MESSAGES_FOR_ANALYSIS=30
MIN_CONFIDENCE_FOR_MATCHING=0.6
MAX_MATCHES_PER_USER=10
